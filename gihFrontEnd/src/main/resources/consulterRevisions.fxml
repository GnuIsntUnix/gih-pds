  <TableView fx:id="table" prefHeight="360.0" prefWidth="600.0">
    <columns>
      <TableColumn fx:id="idCol" prefWidth="50.0" text="ID" />
      <TableColumn fx:id="dateEntreeCol" prefWidth="150.0" text="Date d'entrée" />
      <TableColumn fx:id="kilometrageCol" prefWidth="100.0" text="Kilométrage" />
      <TableColumn fx:id="dateSortieCol" prefWidth="150.0" text="Date de sortie" />
      <TableColumn fx:id="descriptionCol" prefWidth="150.0" text="Description" />
      <TableColumn fx:id="typeRevisionCol" prefWidth="100.0" text="Type de révision" />
      <TableColumn fx:id="actionCol" prefWidth="50.0" text="Action" />
    </columns>
  </TableView>
</VBox>
